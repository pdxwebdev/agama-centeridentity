{
    "secretlocationauth": {
        "projectName": "secretlocationauth",
        "description": "Authenticate by selecting a secret location on a map",
        "type": "community",
        "author": "pdxwebdev",
        "version": "0.0.1",
        "authorWebsite": "https://github.com/pdxwebdev/centeridentityagama",
        "githubUri": "https://github.com/pdxwebdev/centeridentityagama",
        "license": "apache-2.0",
        "lastUpdated": "1709620873942",
        "tags": [
            "openid-connect",
            "authentication"
        ],
        "noDirectLaunch": [],
        "configs": {
            "com.centeridentity.oidc": {
                "com.centeridentity.oidc": {
                    "centeridentity": {
                        "oauth": {
                            "authzEndpoint": "https://centeridentity.com/oauth2/openid-login",
                            "tokenEndpoint": "https://centeridentity.com/oauth2/openid-token",
                            "userInfoEndpoint": "https://api.github.com/oauth2/openid-userinfo",
                            "clientId": "mangled",
                            "clientSecret": "twisted",
                            "scopes": [
                                "user"
                            ]
                        },
                        "provision": {
                            "uidPrefix": "centeridentity-",
                            "attribute": "login"
                        }
                    }
                }
            }
        },
        "name": "secretlocationauth"
    }
